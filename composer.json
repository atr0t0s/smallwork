{
    "name": "smallwork/smallwork",
    "description": "Small footprint full-stack AI framework for PHP",
    "type": "framework",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Smallwork\\": "src/",
            "App\\": "app/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
